"UnlitGeneric"
{
	"$basetexture" "particles/starfox/tracer/angler_cap"
	"$additive" 	1
	"$vertexcolor" 	1
	"$vertexalpha" 	1
	"$translucent" 	1

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 25
		}
	}
}